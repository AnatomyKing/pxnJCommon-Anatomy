<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
                             http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<name>pxnCommon</name>
	<artifactId>common</artifactId>
	<groupId>com.poixson</groupId>
	<version>3.6.2</version>
	<packaging>pom</packaging>
	<url>http://poixson.com</url>
	<scm>
		<url>https://github.com/GrowControl/pxnCommon</url>
		<connection>scm:git:https://github.com/GrowControl/pxnCommon.git</connection>
		<developerConnection>scm:git:git@github.com:GrowControl/pxnCommon.git</developerConnection>
	</scm>
	<ciManagement>
		<url>ci.poixson.com</url>
		<system>pxnci</system>
	</ciManagement>
	<organization>
		<name>PoiXson</name>
		<url>http://poixson.com</url>
	</organization>
	<issueManagement>
		<url>https://github.com/GrowControl/pxnCommon/issues</url>
		<system>github</system>
	</issueManagement>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<modules>
		<module>core</module>
		<module>app</module>
		<module>config</module>
		<module>loggers</module>
		<module>net</module>
		<module>plugin</module>
		<module>scheduler</module>
		<module>shell</module>
	</modules>
	<build>
		<defaultGoal>install</defaultGoal>
	</build>
</project>
